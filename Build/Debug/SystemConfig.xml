<?xml version="1.0"?>
<SystemConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="SystemConfig">
  <!--串口号-->
  <Port>COM8</Port>
  <!--波特率-->
  <BaudRate>9600</BaudRate>
  <!--检测周期-->
  <Preiod>1</Preiod>
  <!--检测单位-->
  <PreiodUnit>0</PreiodUnit>
  <!--主界面曲线X轴显示范围-->
  <Xrange>31</Xrange>
  <!--环境温度-->
  <Temperature>20</Temperature>
  <!--体积-->
  <Volume>1</Volume>
  <!--相对分子量-->
  <Molecular>34</Molecular>
  <!--数据库备份地址-->
  <BackupPath>D:\Backup</BackupPath>
  <!--是否自动备份-->
  <Isbackup>true</Isbackup>
  <!--备份周期-->
  <BackupPreiod>29</BackupPreiod>
  <!--中心站地址-->
  <CenterIP>192.168.0.102</CenterIP>
  <!--中心站端口号-->
  <CenterPort>9001</CenterPort>
  <!--声音文件名称-->
  <SoundName>ALARM1.WAV</SoundName>  
  <User>user</User>
  <UserPWD>111111</UserPWD>
  <Admin>admin</Admin>
  <AdminPWD>1</AdminPWD>
  <!--皮肤名称-->
  <SkinName>Black</SkinName>
  <!--是否自动检测-->
  <Isauto>false</Isauto>
</SystemConfig>